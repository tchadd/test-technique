    "target": "es2016",
    "module": "commonjs",
    "lib": [
      "es2021",
      "dom",
      "dom.iterable"
    ],
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["node"],
    "jsx": "react"
  },
}